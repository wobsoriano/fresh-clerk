{
  "name": "@wobsoriano/fresh-clerk",
  "version": "0.0.3",
  "lock": false,
  "exports": {
    "./plugin.ts": "./src/plugin.ts",
    "./components/SignedIn.tx": "./src/components/SignedIn.tsx",
    "./hooks": "./src/hooks/index.ts",
    "./islands/ClerkProvider.tsx": "./src/islands/ClerkProvider.ts",
    "./islands/SignIn.tsx": "./src/islands/SignIn.ts",
    "./islands/UserButton.tsx": "./src/islands/UserButton.ts",
    "./server": "./src/server/index.ts"
  },
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "start": "deno run -A --watch=playground/routes/ playground/dev.ts",
    "build": "deno run -A playground/dev.ts build",
    "preview": "deno run -A playground/main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "publish": {
    "include": [
      "deno.json",
      "README.md",
      "LICENSE",
      "src/"
    ],
    "exclude": [
      "playground/"
    ]
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.8/",
    "preact": "npm:preact@10.19.6",
    "@preact/signals": "npm:@preact/signals@1.2.2",
    "$std/": "https://deno.land/std@0.216.0/",
    "src/": "./src/",
    "@clerk/backend": "npm:@clerk/backend@1.13.5",
    "@clerk/clerk-js": "npm:@clerk/clerk-js@5.24.0",
    "@clerk/types": "npm:@clerk/types@4.23.0",
    "@clerk/shared": "npm:@clerk/shared@2.8.4",
    "@clerk/shared/": "npm:@clerk/shared@2.8.4/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
