{
  "name": "@wobsoriano/fresh-clerk",
  "version": "0.0.0",
  "lock": false,
  "exports": {
    ".": "./src/mod.ts",
    "./plugin": "./src/plugin.ts",
    "./hooks": "./src/hooks/mod.ts",
    "./components": "./src/components/mod.ts",
    "./islands": "./src/islands/mod.ts",
    "./server": "./src/server/mod.ts"
  },
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "dev": "deno run -A --watch=playground/static/,playground/routes/ playground/dev.ts",
    "build": "deno run -A playground/dev.ts build",
    "start": "deno run -A playground/main.ts",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "fmt": {
    "semiColons": true,
    "singleQuote": true
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "publish": {
    "exclude": [
      "playground",
      ".zed",
      ".github"
    ]
  },
  "imports": {
    "fresh": "jsr:@fresh/core@^2.0.0-alpha.21",
    "preact": "npm:preact@^10.23.2",
    "@preact/signals": "npm:@preact/signals@^1.3.0",
    "src/": "./src/"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head"
    ]
  }
}
