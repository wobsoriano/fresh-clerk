{
  "lock": false,
  "tasks": {
    "dev": "deno run -A --watch=static/,routes/,../src dev.ts dev",
    "build": "deno run -A dev.ts build",
    "start": "deno run -A main.ts",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "imports": {
    "fresh": "jsr:@fresh/core@^2.0.0-alpha.22",
    "preact": "npm:preact@^10.24.2",
    "@preact/signals": "npm:@preact/signals@^1.3.0",
    "fresh-clerk/islands": "../src/islands/mod.ts",
    "fresh-clerk/components": "../src/components/mod.ts",
    "fresh-clerk/server": "../src/server/mod.ts",
    "fresh-clerk/hooks": "../src/hooks/mod.ts",
    "fresh-clerk/plugin": "../src/plugin.ts"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head"
    ]
  }
}
